import java.io.*;
import java.util.Scanner;
public class Main {//첫째 줄에 테스트 케이스의 개수 T가 주어진다. 각 테스트 케이스는 한 줄로 이루어져 있으며, 각 줄에 A와 B가 주어진다. (0 < A, B < 10)
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		Scanner sc = new Scanner(System.in);
		int l = sc.nextInt();
		int a=0,b=0;
		int[] arr = new int[l];
		for(int i=0;i<l;i++) {
			a=sc.nextInt();
		    b=sc.nextInt();
		    arr[i]=a+b;
		}
		for(int i=0;i<l;i++) {
			System.out.println(arr[i]);
		}
	}

}
